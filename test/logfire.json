{
  "redis": {
    "host": "localhost",
    "port": 6379,
    "db": 3,
    "auth": ""
  },

  "ttl_flush_interval": 1,

  "categories": {
    "video": {
      "events": {
        "success": {
          "fields": {
            "provider": {
              "type": "string",
              "required": true
            },
            "video_identifier": {
              "type": "string"
            },
            "server": {
              "type": "number"
            }
          }
        },
        "error": {
          "fields": {
            "code": {
              "type": "string"
            },
            "stack": {
              "type": "string"
            },
            "settings": {
              "type": "string"
            },
            "info": {
              "type": "string"
            },
            "server": {
              "type": "number"
            }
          }
        }
      }
    },
    "cache": {
      "events": {
        "hit": {
          "ttl": 600
        },
        "miss": {}
      }
    }
  }
}

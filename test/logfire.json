{
  "port": 8088,

  "redis": {
    "host": "localhost",
    "port": 6379,
    "db": 3,
    "auth": ""
  },

  "flush_interval": 1,
  "events": {
    "video.success": {
      "fields": {
        "provider": {
          "type": "string",
          "required": true
        },
        "video_identifier": {
          "type": "string"
        },
        "server": {
          "type": "number"
        }
      }
    },
    "video.error": {
      "fields": {
        "code": {
          "type": "string"
        },
        "stack": {
          "type": "string"
        },
        "settings": {
          "type": "string"
        },
        "info": {
          "type": "string"
        },
        "server": {
          "type": "number"
        }
      }
    },
    "cache.hit": {
      "ttl": 600
    },
    "cache.miss": {}
  }
}
